<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns="http://www.julielab.de/conceptdb"
                 xsi:schemaLocation="http://www.julielab.de/conceptdb http://www.julielab.de/conceptdb/conceptdb-1.0.0.xsd
					http://www.julielab.de/conceptdb/operators/serverplugin http://www.julielab.de/conceptdb/operators/serverpluginoperator-1.0.0.xsd">
    <versioning>
        <version>1.0-test</version>
    </versioning>
    <connection>
        <uri>to be set in test</uri>
    </connection>
    <operations>
        <operation xmlns="http://www.julielab.de/conceptdb/operators/serverplugin">
            <operator>de.julielab.concepts.db.core.ServerPluginDatabaseOperator</operator>
            <pluginendpoint>concepts/concept_aggregate_manager/build_aggregates_by_mappings</pluginendpoint>
            <configuration>
                <parameters>
                    <parameter parametername="allowedMappingTypes" tojson="true">
                        <type>EQUAL</type>
                    </parameter>
                    <parameter parametername="aggregatedLabel">MAPPING_AGGREGATE</parameter>
                </parameters>
            </configuration>
        </operation>
    </operations>
</conceptdatabase>