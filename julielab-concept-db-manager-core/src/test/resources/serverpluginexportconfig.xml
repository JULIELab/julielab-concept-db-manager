<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.julielab.de/conceptdb"
                 xsi:schemaLocation="http://www.julielab.de/conceptdb conceptdb.xsd">
    <versioning>
        <version>1.0-test</version>
    </versioning>
    <connection>
        <uri>http://localhost:7474</uri>
    </connection>
    <exports>
        <export>
            <request>
                <rest>
                    <restendpoint>/concepts/export/lingpipe_dictionary</restendpoint>
                    <httpmethod>GET</httpmethod>
                </rest>
                <parameters>
                    <parameter name="label">CONCEPT</parameter>
                    <parameter name="id_property">sourceIds</parameter>
                </parameters>
            </request>
            <decoding>
                <json2bytearray>false</json2bytearray>
                <base64>true</base64>
                <gzip>true</gzip>
            </decoding>
            <outputfile>
                src/test/resources/output/serverplugindict.txt
            </outputfile>
        </export>
    </exports>
</conceptdatabase>