<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns="http://www.julielab.de/conceptdb"
                 xsi:schemaLocation="http://www.julielab.de/conceptdb http://www.julielab.de/conceptdb/conceptdb-1.0.0.xsd
					http://www.julielab.de/conceptdb/exporters/serverplugin http://www.julielab.de/conceptdb/exporters/serverpluginexporter-1.0.0.xsd">
    <versioning>
        <version>1.0-test</version>
    </versioning>
    <connection>
        <uri>http://localhost:7474</uri>
    </connection>
    <exports>
        <export xmlns="http://www.julielab.de/conceptdb/exporters/serverplugin">
            <exporter>ServerPluginExporter</exporter>
                <pluginname>
                    Export
                </pluginname>
                <pluginendpoint>
                    lingpipe_dictionary
                </pluginendpoint>
            <configuration>
                <outputfile>
                    src/test/resources/output/serverplugindict.txt
                </outputfile>
                <decoding>
                    <json2bytearray>false</json2bytearray>
                    <base64>true</base64>
                    <gzip>true</gzip>
                </decoding>
                <parameters>
                    <parameter parametername="label">CONCEPT</parameter>
                    <parameter parametername="id_property">
                        <property>sourceIds</property>
                    </parameter>
                </parameters>
            </configuration>
        </export>
    </exports>
</conceptdatabase>