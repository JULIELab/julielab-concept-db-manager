<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 xmlns="http://www.julielab.de/conceptdb"
				 xsi:schemaLocation="http://www.julielab.de/conceptdb http://www.julielab.de/conceptdb/conceptdb-1.0.0.xsd
					http://www.julielab.de/conceptdb/exporters/serverpluginexporter http://www.julielab.de/conceptdb/exporters/serverpluginexporter-1.0.0.xsd">
	<versioning>
		<version>1.0-test</version>
	</versioning>
	<connection>
		<uri>http://localhost:7474</uri>
		<user></user>
		<password></password>
	</connection>
	<exports>
		<export xmlns="http://www.julielab.de/conceptdb/exporters/serverpluginexporter">
			<exporter>ServerPluginExporter</exporter>
			<configuration>
				<pluginname>
					Export
				</pluginname>
				<pluginendpoint>
					lingpipe_dictionary
				</pluginendpoint>
				<outputfile>
					src/test/resources/output/dict.txt
				</outputfile>
				<decoding>
					<json2bytearray>false</json2bytearray>
					<base64>true</base64>
					<gzip>true</gzip>
				</decoding>
				<parameters>
					<conceptlabel parametername="label">CONCEPT</conceptlabel>
					<id_property>
						<property>sourceIds</property>
					</id_property>
				</parameters>
			</configuration>
		</export>
	</exports>
</conceptdatabase>