<?xml version="1.0" encoding="UTF-8"?>

<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.julielab.de/conceptdb"
                 xsi:schemaLocation="http://www.julielab.de/conceptdb conceptdb.xsd">
    <versioning>
        <version>1.0-test</version>
    </versioning>
    <connection>
        <uri>http://sonstwas.muh</uri>
    </connection>
    <exports>
        <export>
            <request>
                <rest>
                    <restendpoint>/db/data/ext/Export/graphdb/hypernyms</restendpoint>
                    <httpmethod>GET</httpmethod>
                </rest>
                <parameters>
                    <parameter name="label" javatype="java.lang.String">
                        ID_MAP_NCBI_GENES
                    </parameter>
                    <parameter name="labels" toescapedjson="true" elementtype="java.lang.String">
                        <arrayitem>FACET</arrayitem>
                        <arrayitem>BIO_PORTAL</arrayitem>
                    </parameter>
                </parameters>
            </request>
            <outputfile>output</outputfile>
        </export>
    </exports>
</conceptdatabase>