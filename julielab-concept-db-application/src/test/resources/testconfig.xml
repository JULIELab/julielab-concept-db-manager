<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns="http://www.julielab.de/conceptdb"
                 xsi:schemaLocation="http://www.julielab.de/conceptdb http://www.julielab.de/conceptdb-1.0.0.xsd
                 http://www.julielab.de/conceptdb/concepts/bioportal http://www.julielab.de/conceptdb/concepts/bioportalconcepts-1.0.0.xsd
                 http://www.julielab.de/conceptdb/facets/bioportal http://www.julielab.de/conceptdb/facets/bioportalfacets-1.0.0.xsd
                 http://www.julielab.de/conceptdb/exporters/serverplugin http://www.julielab.de/conceptdb/exporters/serverpluginexporter-1.0.0.xsd">
    <versioning>
        <version>1.0</version>
    </versioning>
    <connection>
        <uri>src/test/resources/graphdb</uri>
    </connection>
    <imports>
        <import>
            <concepts>
                <creator xmlns="http://www.julielab.de/conceptdb/concepts/bioportal">
                    <name>JulielabBioPortalToolsConceptCreator</name>
                    <configuration>
                        <path>src/test/resources/PO.cls.jsonlst.gz</path>
                    </configuration>
                </creator>
            </concepts>
            <facet>
                <creator xmlns="http://www.julielab.de/conceptdb/facets/bioportal">
                    <configuration>
                        <facetgroup>
                            <name>BioPortal</name>
                        </facetgroup>
                    </configuration>
                </creator>
            </facet>
        </import>
    </imports>
</conceptdatabase>