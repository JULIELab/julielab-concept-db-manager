<conceptdatabase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns="conceptdb-1.1.0.xsd"
                 xsi:schemaLocation="conceptdb-1.1.0.xsd conceptdb-1.1.0.xsd
                 http://www.julielab.de/conceptdb/operators/cypher http://www.julielab.de/conceptdb/operators/cypheroperator-1.0.0.xsd">
    <versioning>
        <version>1.0</version>
    </versioning>
    <connection>
        <uri>src/test/resources/graphdb</uri>
    </connection>
    <operations>
        <operation name="antherwall">
            <request>
                <cypherquery>MATCH (c:CONCEPT) WHERE "http://purl.obolibrary.org/obo/PO_0000002" IN c.sourceIds SET
                    c.myprop="NEWVAL" RETURN c.myprop
                </cypherquery>
            </request>
        </operation>
        <operation name="NOT_CALLED">
            <request>
                <cypherquery>totally invalid query which doesn't matter because this isn't called anyway</cypherquery>
            </request>
        </operation>
    </operations>
</conceptdatabase>